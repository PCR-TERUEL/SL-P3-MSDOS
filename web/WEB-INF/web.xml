<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd"
           version="4.0">


    <servlet>
        <servlet-name>GetAllGames</servlet-name>
        <servlet-class>dao.GetAllGames</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetAllGames</servlet-name>
        <url-pattern>/get_all_games</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SearchGame</servlet-name>
        <servlet-class>dao.SearchGames</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchGame</servlet-name>
        <url-pattern>/search_game</url-pattern>
    </servlet-mapping>
</web-app>
